{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["/sitemap.xml"]
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html"
        }
    },
    "globalHeaders": {
        "Cache-Control": "public, max-age=3600, s-maxage=86400"
    },
    "routes": [
        {
            "route": "/sitemap.xml",
            "allowedRoles": ["anonymous"]
        },
        {
            "route": "/*.js",
            "headers": {
                "Cache-Control": "public, max-age=31536000, s-maxage=31536000, immutable"
            }
        },
        {
            "route": "/*.css",
            "headers": {
                "Cache-Control": "public, max-age=31536000, s-maxage=31536000, immutable"
            }
        },
        {
            "route": "/index.html",
            "headers": {
                "Cache-Control": "public, max-age=0, s-maxage=3600, must-revalidate"
            }
        },
        {
            "route": "/main.*.js",
            "headers": {
                "Cache-Control": "public, max-age=31536000, s-maxage=31536000, immutable"
            }
        }
    ]
}