{
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html"
        }
    },
    "globalHeaders": {
        "Cache-Control": "public, max-age=3600, s-maxage=86400"
    },
    "routes": [
        {
            "route": "/index.html",
            "headers": {
                "Cache-Control": "public, max-age=0, s-maxage=0, must-revalidate, no-store",
                "Pragma": "no-cache",
                "Expires": "0"
            }
        },
        {
            "route": "/*.js",
            "headers": {
                "Cache-Control": "public, max-age=31536000, s-maxage=31536000, immutable"
            }
        },
        {
            "route": "/*.css",
            "headers": {
                "Cache-Control": "public, max-age=31536000, s-maxage=31536000, immutable"
            }
        },
        {
            "route": "/sitemap.xml",
            "headers": {
                "Cache-Control": "public, max-age=3600, s-maxage=86400"
            }
        },
        {
            "route": "/main.*.js",
            "headers": {
                "Cache-Control": "public, max-age=31536000, s-maxage=31536000, immutable"
            }
        }
    ]
}